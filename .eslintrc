{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "globals": {
    "jQuery": true,
    "$": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true,
    "browser": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-console": 1
  }
}
